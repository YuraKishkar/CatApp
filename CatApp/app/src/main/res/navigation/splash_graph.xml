<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/spash_graph"
            app:startDestination="@id/splashFragment">

    <fragment android:id="@+id/splashFragment"
              android:name="com.example.catapp.view.fragment.splashFragment.SplashFragment"
              android:label="fragment_splash"
              tools:layout="@layout/fragment_splash">
        <action android:id="@+id/action_splashFragment_to_mainActivity" app:destination="@id/mainActivity"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
        />
    </fragment>
    <activity android:id="@+id/mainActivity" android:name="com.example.catapp.view.activity.MainActivity"
              android:label="activity_main" tools:layout="@layout/activity_main"/>
</navigation>